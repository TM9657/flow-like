{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:path:default",
		"core:event:default",
		"core:window:default",
		"core:app:default",
		"core:image:default",
		"core:resources:default",
		"core:menu:default",
		"core:tray:default",
		"dialog:allow-ask",
		"dialog:allow-confirm",
		"dialog:allow-message",
		"dialog:allow-open",
		"dialog:allow-save",
		"shell:allow-open",
		"shell:allow-execute",
		"shell:allow-kill",
		"shell:allow-spawn",
		"shell:allow-stdin-write",
		"shell:default",
		"dialog:default",
		"deep-link:default"
	]
}
