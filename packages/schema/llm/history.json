{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "History",
	"type": "object",
	"required": ["messages", "model"],
	"properties": {
		"frequency_penalty": {
			"type": ["number", "null"],
			"format": "float"
		},
		"max_completion_tokens": {
			"type": ["integer", "null"],
			"format": "uint32",
			"minimum": 0.0
		},
		"messages": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/HistoryMessage"
			}
		},
		"model": {
			"type": "string"
		},
		"n": {
			"type": ["integer", "null"],
			"format": "uint32",
			"minimum": 0.0
		},
		"presence_penalty": {
			"type": ["number", "null"],
			"format": "float"
		},
		"response_format": {
			"anyOf": [
				{
					"$ref": "#/definitions/ResponseFormat"
				},
				{
					"type": "null"
				}
			]
		},
		"seed": {
			"type": ["integer", "null"],
			"format": "uint32",
			"minimum": 0.0
		},
		"stop": {
			"type": ["array", "null"],
			"items": {
				"type": "string"
			}
		},
		"stream": {
			"type": ["boolean", "null"]
		},
		"stream_options": {
			"anyOf": [
				{
					"$ref": "#/definitions/StreamOptions"
				},
				{
					"type": "null"
				}
			]
		},
		"temperature": {
			"type": ["number", "null"],
			"format": "float"
		},
		"tool_choice": {
			"anyOf": [
				{
					"$ref": "#/definitions/ToolChoice"
				},
				{
					"type": "null"
				}
			]
		},
		"tools": {
			"type": ["array", "null"],
			"items": {
				"$ref": "#/definitions/Tool"
			}
		},
		"top_p": {
			"type": ["number", "null"],
			"format": "float"
		},
		"user": {
			"type": ["string", "null"]
		}
	},
	"definitions": {
		"Content": {
			"anyOf": [
				{
					"type": "object",
					"required": ["text", "type"],
					"properties": {
						"text": {
							"type": "string"
						},
						"type": {
							"$ref": "#/definitions/ContentType"
						}
					}
				},
				{
					"type": "object",
					"required": ["data", "mime_type", "type"],
					"properties": {
						"data": {
							"type": "string"
						},
						"mime_type": {
							"type": "string"
						},
						"type": {
							"$ref": "#/definitions/ContentType"
						}
					}
				}
			]
		},
		"ContentType": {
			"type": "string",
			"enum": ["text", "image_url"]
		},
		"HistoryFunction": {
			"type": "object",
			"required": ["name", "parameters"],
			"properties": {
				"description": {
					"type": ["string", "null"]
				},
				"name": {
					"type": "string"
				},
				"parameters": {
					"$ref": "#/definitions/HistoryFunctionParameters"
				}
			}
		},
		"HistoryFunctionParameters": {
			"type": "object",
			"required": ["type"],
			"properties": {
				"properties": {
					"type": ["object", "null"],
					"additionalProperties": {
						"$ref": "#/definitions/HistoryJSONSchemaDefine"
					}
				},
				"required": {
					"type": ["array", "null"],
					"items": {
						"type": "string"
					}
				},
				"type": {
					"$ref": "#/definitions/HistoryJSONSchemaType"
				}
			}
		},
		"HistoryJSONSchemaDefine": {
			"type": "object",
			"properties": {
				"description": {
					"type": ["string", "null"]
				},
				"enum_values": {
					"type": ["array", "null"],
					"items": {
						"type": "string"
					}
				},
				"items": {
					"anyOf": [
						{
							"$ref": "#/definitions/HistoryJSONSchemaDefine"
						},
						{
							"type": "null"
						}
					]
				},
				"properties": {
					"type": ["object", "null"],
					"additionalProperties": {
						"$ref": "#/definitions/HistoryJSONSchemaDefine"
					}
				},
				"required": {
					"type": ["array", "null"],
					"items": {
						"type": "string"
					}
				},
				"type": {
					"anyOf": [
						{
							"$ref": "#/definitions/HistoryJSONSchemaType"
						},
						{
							"type": "null"
						}
					]
				}
			}
		},
		"HistoryJSONSchemaType": {
			"type": "string",
			"enum": ["object", "number", "string", "array", "null", "boolean"]
		},
		"HistoryMessage": {
			"type": "object",
			"required": ["content", "role"],
			"properties": {
				"content": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Content"
					}
				},
				"name": {
					"type": ["string", "null"]
				},
				"role": {
					"$ref": "#/definitions/Role"
				},
				"tool_call_id": {
					"type": ["string", "null"]
				},
				"tool_calls": {
					"type": ["array", "null"],
					"items": {
						"$ref": "#/definitions/ToolCall"
					}
				}
			}
		},
		"ResponseFormat": {
			"anyOf": [
				{
					"type": "string"
				},
				true
			]
		},
		"Role": {
			"type": "string",
			"enum": ["system", "user", "assistant", "function", "tool"]
		},
		"StreamOptions": {
			"type": "object",
			"required": ["include_usage"],
			"properties": {
				"include_usage": {
					"type": "boolean"
				}
			}
		},
		"Tool": {
			"type": "object",
			"required": ["function", "type"],
			"properties": {
				"function": {
					"$ref": "#/definitions/HistoryFunction"
				},
				"type": {
					"$ref": "#/definitions/ToolType"
				}
			}
		},
		"ToolCall": {
			"type": "object",
			"required": ["function", "id", "type"],
			"properties": {
				"function": {
					"$ref": "#/definitions/ToolCallFunction"
				},
				"id": {
					"type": "string"
				},
				"type": {
					"type": "string"
				}
			}
		},
		"ToolCallFunction": {
			"type": "object",
			"properties": {
				"arguments": {
					"type": ["string", "null"]
				},
				"name": {
					"type": ["string", "null"]
				}
			}
		},
		"ToolChoice": {
			"anyOf": [
				{
					"type": "null"
				},
				{
					"type": "null"
				},
				{
					"type": "null"
				},
				{
					"type": "object",
					"required": ["function", "type"],
					"properties": {
						"function": {
							"$ref": "#/definitions/HistoryFunction"
						},
						"type": {
							"$ref": "#/definitions/ToolType"
						}
					}
				}
			]
		},
		"ToolType": {
			"type": "string",
			"enum": ["function"]
		}
	}
}
