{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Response",
	"type": "object",
	"required": ["choices", "usage"],
	"properties": {
		"id": {
			"type": ["string", "null"]
		},
		"choices": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Choice"
			}
		},
		"created": {
			"type": ["integer", "null"],
			"format": "uint64",
			"minimum": 0.0
		},
		"model": {
			"type": ["string", "null"]
		},
		"service_tier": {
			"type": ["string", "null"]
		},
		"system_fingerprint": {
			"type": ["string", "null"]
		},
		"object": {
			"type": ["string", "null"]
		},
		"usage": {
			"$ref": "#/definitions/Usage"
		}
	},
	"definitions": {
		"Choice": {
			"type": "object",
			"required": ["finish_reason", "index", "message"],
			"properties": {
				"index": {
					"type": "integer",
					"format": "int32"
				},
				"finish_reason": {
					"type": "string"
				},
				"message": {
					"$ref": "#/definitions/ResponseMessage"
				},
				"logprobs": {
					"anyOf": [
						{
							"$ref": "#/definitions/LogProbs"
						},
						{
							"type": "null"
						}
					]
				}
			}
		},
		"ResponseMessage": {
			"type": "object",
			"required": ["role"],
			"properties": {
				"role": {
					"type": "string"
				},
				"content": {
					"type": ["string", "null"]
				},
				"refusal": {
					"type": ["string", "null"]
				},
				"tool_calls": {
					"default": [],
					"type": "array",
					"items": {
						"$ref": "#/definitions/FunctionCall"
					}
				}
			}
		},
		"FunctionCall": {
			"type": "object",
			"required": ["function", "id"],
			"properties": {
				"index": {
					"type": ["integer", "null"],
					"format": "int32"
				},
				"id": {
					"type": "string"
				},
				"type": {
					"type": ["string", "null"]
				},
				"function": {
					"$ref": "#/definitions/ResponseFunction"
				}
			}
		},
		"ResponseFunction": {
			"type": "object",
			"properties": {
				"name": {
					"type": ["string", "null"]
				},
				"arguments": {
					"type": ["string", "null"]
				}
			}
		},
		"LogProbs": {
			"type": "object",
			"properties": {
				"content": {
					"type": ["array", "null"],
					"items": {
						"$ref": "#/definitions/TokenLogProbs"
					}
				},
				"refusal": {
					"type": ["array", "null"],
					"items": {
						"$ref": "#/definitions/TokenLogProbs"
					}
				}
			}
		},
		"TokenLogProbs": {
			"type": "object",
			"required": ["logprob", "token"],
			"properties": {
				"token": {
					"type": "string"
				},
				"logprob": {
					"type": "number",
					"format": "double"
				},
				"bytes": {
					"type": ["array", "null"],
					"items": {
						"type": "integer",
						"format": "uint8",
						"minimum": 0.0
					}
				},
				"top_logprobs": {
					"type": ["array", "null"],
					"items": {
						"$ref": "#/definitions/TopLogProbs"
					}
				}
			}
		},
		"TopLogProbs": {
			"type": "object",
			"required": ["logprob", "token"],
			"properties": {
				"token": {
					"type": "string"
				},
				"logprob": {
					"type": "number",
					"format": "double"
				},
				"bytes": {
					"type": ["array", "null"],
					"items": {
						"type": "integer",
						"format": "uint8",
						"minimum": 0.0
					}
				}
			}
		},
		"Usage": {
			"type": "object",
			"required": ["completion_tokens", "prompt_tokens", "total_tokens"],
			"properties": {
				"completion_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"prompt_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"total_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"prompt_tokens_details": {
					"anyOf": [
						{
							"$ref": "#/definitions/PromptTokenDetails"
						},
						{
							"type": "null"
						}
					]
				},
				"completion_tokens_details": {
					"anyOf": [
						{
							"$ref": "#/definitions/CompletionTokenDetails"
						},
						{
							"type": "null"
						}
					]
				}
			}
		},
		"PromptTokenDetails": {
			"type": "object",
			"required": ["audio_tokens", "cached_tokens"],
			"properties": {
				"cached_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"audio_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				}
			}
		},
		"CompletionTokenDetails": {
			"type": "object",
			"required": [
				"accepted_prediction_tokens",
				"audio_tokens",
				"reasoning_tokens",
				"rejected_prediction_tokens"
			],
			"properties": {
				"accepted_prediction_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"audio_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"reasoning_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				},
				"rejected_prediction_tokens": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0.0
				}
			}
		}
	}
}
