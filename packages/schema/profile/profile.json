		"bits",
		"created",
		"description",
		"name",
		"thumbnail",
		"updated"
	],
	"properties": {
		"id": {
			"default": "ajopvqg5t9ap8prhr1evglu9",
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"thumbnail": {
			"type": "string"
		},
		"hub": {
			"default": "",
