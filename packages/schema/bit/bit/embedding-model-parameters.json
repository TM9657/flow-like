{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "EmbeddingModelProvider",
	"type": "object",
	"properties": {
		"languages": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"vector_length": {
			"type": "integer",
			"format": "uint32",
			"minimum": 0
		},
		"input_length": {
			"type": "integer",
			"format": "uint32",
			"minimum": 0
		},
		"prefix": {
			"$ref": "#/$defs/Prefix"
		},
		"pooling": {
			"$ref": "#/$defs/Pooling"
		},
		"provider": {
			"$ref": "#/$defs/ModelProvider"
		}
	},
	"required": [
		"languages",
		"vector_length",
		"input_length",
		"prefix",
		"pooling",
		"provider"
	],
	"$defs": {
		"Prefix": {
			"type": "object",
			"properties": {
				"query": {
					"type": "string"
				},
				"paragraph": {
					"type": "string"
				}
			},
			"required": ["query", "paragraph"]
		},
		"Pooling": {
			"type": "string",
			"enum": ["CLS", "Mean", "None"]
		},
		"ModelProvider": {
			"type": "object",
			"properties": {
				"provider_name": {
					"type": "string"
				},
				"model_id": {
					"type": ["string", "null"]
				},
				"version": {
					"type": ["string", "null"]
				},
				"params": {
					"type": ["object", "null"],
					"additionalProperties": true
				}
			},
			"required": ["provider_name"]
		}
	}
}
