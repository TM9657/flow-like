{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Event",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"board_id": {
			"type": "string"
		},
		"board_version": {
			"type": ["array", "null"],
			"prefixItems": [
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				},
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				},
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				}
			],
			"minItems": 3,
			"maxItems": 3
		},
		"node_id": {
			"type": "string"
		},
		"variables": {
			"type": "object",
			"additionalProperties": {
				"$ref": "#/$defs/Variable"
			}
		},
		"config": {
			"type": "array",
			"items": {
				"type": "integer",
				"format": "uint8",
				"minimum": 0,
				"maximum": 255
			}
		},
		"active": {
			"type": "boolean"
		},
		"canary": {
			"anyOf": [
				{
					"$ref": "#/$defs/CanaryEvent"
				},
				{
					"type": "null"
				}
			]
		},
		"priority": {
			"type": "integer",
			"format": "uint32",
			"minimum": 0
		},
		"event_type": {
			"type": "string"
		},
		"notes": {
			"anyOf": [
				{
					"$ref": "#/$defs/ReleaseNotes"
				},
				{
					"type": "null"
				}
			]
		},
		"event_version": {
			"type": "array",
			"prefixItems": [
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				},
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				},
				{
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				}
			],
			"minItems": 3,
			"maxItems": 3
		},
		"created_at": {
			"$ref": "#/$defs/SystemTime"
		},
		"updated_at": {
			"$ref": "#/$defs/SystemTime"
		}
	},
	"required": [
		"id",
		"name",
		"description",
		"board_id",
		"node_id",
		"variables",
		"config",
		"active",
		"priority",
		"event_type",
		"event_version",
		"created_at",
		"updated_at"
	],
	"$defs": {
		"Variable": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"category": {
					"type": ["string", "null"]
				},
				"description": {
					"type": ["string", "null"]
				},
				"default_value": {
					"type": ["array", "null"],
					"items": {
						"type": "integer",
						"format": "uint8",
						"minimum": 0,
						"maximum": 255
					}
				},
				"data_type": {
					"$ref": "#/$defs/VariableType"
				},
				"value_type": {
					"$ref": "#/$defs/ValueType"
				},
				"exposed": {
					"type": "boolean"
				},
				"secret": {
					"type": "boolean"
				},
				"editable": {
					"type": "boolean"
				},
				"hash": {
					"type": ["integer", "null"],
					"format": "uint64",
					"minimum": 0
				}
			},
			"required": [
				"id",
				"name",
				"data_type",
				"value_type",
				"exposed",
				"secret",
				"editable"
			]
		},
		"VariableType": {
			"type": "string",
			"enum": [
				"Execution",
				"String",
				"Integer",
				"Float",
				"Boolean",
				"Date",
				"PathBuf",
				"Generic",
				"Struct",
				"Byte"
			]
		},
		"ValueType": {
			"type": "string",
			"enum": ["Array", "Normal", "HashMap", "HashSet"]
		},
		"CanaryEvent": {
			"type": "object",
			"properties": {
				"weight": {
					"type": "number",
					"format": "float"
				},
				"variables": {
					"type": "object",
					"additionalProperties": {
						"$ref": "#/$defs/Variable"
					}
				},
				"board_id": {
					"type": "string"
				},
				"board_version": {
					"type": ["array", "null"],
					"prefixItems": [
						{
							"type": "integer",
							"format": "uint32",
							"minimum": 0
						},
						{
							"type": "integer",
							"format": "uint32",
							"minimum": 0
						},
						{
							"type": "integer",
							"format": "uint32",
							"minimum": 0
						}
					],
					"minItems": 3,
					"maxItems": 3
				},
				"node_id": {
					"type": "string"
				},
				"created_at": {
					"$ref": "#/$defs/SystemTime"
				},
				"updated_at": {
					"$ref": "#/$defs/SystemTime"
				}
			},
			"required": [
				"weight",
				"variables",
				"board_id",
				"node_id",
				"created_at",
				"updated_at"
			]
		},
		"SystemTime": {
			"type": "object",
			"properties": {
				"secs_since_epoch": {
					"type": "integer",
					"format": "uint64",
					"minimum": 0
				},
				"nanos_since_epoch": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				}
			},
			"required": ["secs_since_epoch", "nanos_since_epoch"]
		},
		"ReleaseNotes": {
			"oneOf": [
				{
					"type": "object",
					"properties": {
						"NOTES": {
							"type": "string"
						}
					},
					"required": ["NOTES"],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"URL": {
							"type": "string"
						}
					},
					"required": ["URL"],
					"additionalProperties": false
				}
			]
		}
	}
}
