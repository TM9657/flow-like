			"$ref": "#/$defs/RemoveNodeCommand",
			"required": ["command_type"]
		},
		{
			"type": "object",
			"properties": {
				"command_type": {
					"type": "string",
					"const": "UpdateNode"
				}
			},
			"$ref": "#/$defs/UpdateNodeCommand",
			"required": ["command_type"]
		},
		{
			"type": "object",
