	"$defs": {
		"Comment": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"author": {
					"type": ["string", "null"]
				},
				"content": {
					"type": "string"
				},
				"comment_type": {
					"$ref": "#/$defs/CommentType"
				},
				"timestamp": {
					"$ref": "#/$defs/SystemTime"
				},
				"coordinates": {
					"type": "array",
					"prefixItems": [
						{
							"type": "number",
							"format": "float"
						},
						{
							"type": "number",
							"format": "float"
						},
						{
							"type": "number",
							"format": "float"
						}
					],
					"minItems": 3,
					"maxItems": 3
				},
				"width": {
					"type": ["number", "null"],
					"format": "float"
				},
				"height": {
					"type": ["number", "null"],
					"format": "float"
				},
				"layer": {
					"type": ["string", "null"]
				},
				"color": {
					"type": ["string", "null"]
				},
				"z_index": {
					"type": ["integer", "null"],
					"format": "int32"
				},
				"hash": {
					"type": ["integer", "null"],
					"format": "uint64",
					"minimum": 0
				},
				"is_locked": {
					"type": ["boolean", "null"]
				}
			},
			"required": ["id", "content", "comment_type", "timestamp", "coordinates"]
		},
		"CommentType": {
			"type": "string",
			"enum": ["Text", "Image", "Video"]
		},
		"SystemTime": {
			"type": "object",
			"properties": {
				"secs_since_epoch": {
					"type": "integer",
					"format": "uint64",
					"minimum": 0
				},
				"nanos_since_epoch": {
					"type": "integer",
					"format": "uint32",
					"minimum": 0
				}
			},
			"required": ["secs_since_epoch", "nanos_since_epoch"]
		},
		"RemoveCommentCommand": {
			"type": "object",
			"properties": {
				"comment": {
					"$ref": "#/$defs/Comment"
				}
			},
			"required": ["comment"]
		},
		"UpsertCommentCommand": {
			"type": "object",
			"properties": {
				"comment": {
					"$ref": "#/$defs/Comment"
				},
				"old_comment": {
					"anyOf": [
						{
							"$ref": "#/$defs/Comment"
						},
						{
							"type": "null"
						}
					]
				},
				"current_layer": {
					"type": ["string", "null"]
				}
			},
			"required": ["comment"]
		},
		"Node": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"friendly_name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"coordinates": {
					"type": ["array", "null"],
					"prefixItems": [
						{
							"type": "number",
							"format": "float"
						},
						{
							"type": "number",
							"format": "float"
						},
						{
							"type": "number",
							"format": "float"
						}
					],
					"minItems": 3,
					"maxItems": 3
				},
				"category": {
					"type": "string"
				},
				"scores": {
					"anyOf": [
						{
							"$ref": "#/$defs/NodeScores"
						},
						{
							"type": "null"
						}
					]
				},
				"pins": {
					"type": "object",
					"additionalProperties": {
						"$ref": "#/$defs/Pin"
					}
				},
				"start": {
					"type": ["boolean", "null"]
				},
				"icon": {
					"type": ["string", "null"]
				},
				"comment": {
					"type": ["string", "null"]
				},
				"long_running": {
					"type": ["boolean", "null"]
				},
				"error": {
					"type": ["string", "null"]
				},
				"docs": {
					"type": ["string", "null"]
				},
				"event_callback": {
					"type": ["boolean", "null"]
				},
				"layer": {
					"type": ["string", "null"]
				},
				"hash": {
					"type": ["integer", "null"],
					"format": "uint64",
					"minimum": 0
				},
				"fn_refs": {
					"anyOf": [
						{
							"$ref": "#/$defs/FnRefs"
						},
						{
							"type": "null"
						}
					]
				}
			},
			"required": [
				"id",
				"name",
				"friendly_name",
				"description",
				"category",
				"pins"
			]
		},
		"NodeScores": {
			"description": "Represents quality metrics for a node, with scores ranging from 0 to 10.\nHigher scores indicate worse performance in each category.\n\n# Score Categories\n* `privacy` - Measures data protection and confidentiality level\n* `security` - Assesses resistance against potential attacks\n* `performance` - Evaluates computational efficiency and speed\n* `governance` - Indicates compliance with policies and regulations",
			"type": "object",
			"properties": {
				"privacy": {
					"type": "integer",
					"format": "uint8",
					"minimum": 0,
					"maximum": 255
				},
				"security": {
					"type": "integer",
					"format": "uint8",
					"minimum": 0,
					"maximum": 255
				},
				"performance": {
					"type": "integer",
					"format": "uint8",
					"minimum": 0,
					"maximum": 255
				},
				"governance": {
					"type": "integer",
					"format": "uint8",
					"minimum": 0,
					"maximum": 255
				}
			},
			"required": ["privacy", "security", "performance", "governance"]
		},
		"Pin": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"friendly_name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"pin_type": {
					"$ref": "#/$defs/PinType"
				},
				"data_type": {
					"$ref": "#/$defs/VariableType"
				},
				"schema": {
					"type": ["string", "null"]
				},
				"value_type": {
					"$ref": "#/$defs/ValueType"
				},
				"depends_on": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"connected_to": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"default_value": {
					"type": ["array", "null"],
					"items": {
						"type": "integer",
						"format": "uint8",
						"minimum": 0,
						"maximum": 255
					}
				},
				"index": {
					"type": "integer",
					"format": "uint16",
					"minimum": 0,
					"maximum": 65535
				},
				"options": {
					"anyOf": [
						{
							"$ref": "#/$defs/PinOptions"
						},
						{
							"type": "null"
						}
					]
				}
			},
			"required": [
				"id",
				"name",
				"friendly_name",
				"description",
				"pin_type",
				"data_type",
				"value_type",
				"depends_on",
				"connected_to",
				"index"
			]
		},
		"PinType": {
			"type": "string",
			"enum": ["Input", "Output"]
		},
		"VariableType": {
			"type": "string",
			"enum": [
				"Execution",
				"String",
				"Integer",
				"Float",
				"Boolean",
				"Date",
				"PathBuf",
				"Generic",
				"Struct",
				"Byte"
			]
		},
		"ValueType": {
			"type": "string",
			"enum": ["Array", "Normal", "HashMap", "HashSet"]
		},
		"PinOptions": {
			"type": "object",
			"properties": {
				"sensitive": {
					"type": ["boolean", "null"]
				},
				"valid_values": {
					"type": ["array", "null"],
					"items": {
						"type": "string"
					}
				},
				"range": {
					"type": ["array", "null"],
					"prefixItems": [
						{
							"type": "number",
							"format": "double"
						},
						{
							"type": "number",
							"format": "double"
						}
					],
					"minItems": 2,
					"maxItems": 2
				},
				"step": {
					"type": ["number", "null"],
					"format": "double"
				},
				"enforce_schema": {
					"type": ["boolean", "null"]
				},
				"enforce_generic_value_type": {
					"type": ["boolean", "null"]
				}
			}
		},
		"FnRefs": {
			"type": "object",
			"properties": {
				"fn_refs": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"can_reference_fns": {
					"type": "boolean"
				},
				"can_be_referenced_by_fns": {
					"type": "boolean"
				}
			},
			"required": ["fn_refs", "can_reference_fns", "can_be_referenced_by_fns"]
		},
		"AddNodeCommand": {
			"type": "object",
