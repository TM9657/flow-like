{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "LogMeta",
	"type": "object",
	"properties": {
		"app_id": {
			"type": "string"
		},
		"run_id": {
			"type": "string"
		},
		"board_id": {
			"type": "string"
		},
		"start": {
			"type": "integer",
			"format": "uint64",
			"minimum": 0
		},
		"end": {
			"type": "integer",
			"format": "uint64",
			"minimum": 0
		},
		"log_level": {
			"type": "integer",
			"format": "uint8",
			"minimum": 0,
			"maximum": 255
		},
		"version": {
			"type": "string"
		},
		"nodes": {
			"type": ["array", "null"],
			"items": {
				"type": "array",
				"prefixItems": [
					{
						"type": "string"
					},
					{
						"type": "integer",
						"format": "uint8",
						"minimum": 0,
						"maximum": 255
					}
				],
				"minItems": 2,
				"maxItems": 2
			}
		},
		"logs": {
			"type": ["integer", "null"],
			"format": "uint64",
			"minimum": 0
		},
		"node_id": {
			"type": "string"
		},
		"event_version": {
			"type": ["string", "null"]
		},
		"event_id": {
			"type": "string"
		},
		"payload": {
			"type": "array",
			"items": {
				"type": "integer",
				"format": "uint8",
				"minimum": 0,
				"maximum": 255
			}
		}
	},
	"required": [
		"app_id",
		"run_id",
		"board_id",
		"start",
		"end",
		"log_level",
		"version",
		"node_id",
		"event_id",
		"payload"
	]
}
