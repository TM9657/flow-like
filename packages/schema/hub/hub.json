{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Hub",
	"type": "object",
	"required": [
		"dependencies",
		"description",
		"domain",
		"icon",
		"name",
		"thumbnail"
	],
	"properties": {
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"thumbnail": {
			"type": "string"
		},
		"icon": {
			"type": "string"
		},
		"domain": {
			"type": "string"
		},
		"dependencies": {
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	}
}
