    "dependencies",
    "description",
    "domain",
    "icon",
    "name",
    "thumbnail"
  ],
  "properties": {
    "dependencies": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "description": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "thumbnail": {
      "type": "string"
    }
  }
}
