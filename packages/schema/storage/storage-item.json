{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "StorageItem",
	"type": "object",
	"properties": {
		"location": {
			"type": "string"
		},
		"last_modified": {
			"type": "string"
		},
		"size": {
			"type": "integer",
			"format": "uint",
			"minimum": 0
		},
		"e_tag": {
			"type": ["string", "null"]
		},
		"version": {
			"type": ["string", "null"]
		},
		"is_dir": {
			"type": "boolean"
		}
	},
	"required": ["location", "last_modified", "size", "is_dir"]
}
