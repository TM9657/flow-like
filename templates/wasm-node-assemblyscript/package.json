{
	"name": "flow-like-wasm-node-ts",
	"version": "0.1.0",
	"description": "Template for creating Flow-Like WASM nodes in TypeScript/AssemblyScript",
	"main": "build/release.wasm",
	"type": "module",
	"scripts": {
		"asbuild:debug": "asc assembly/index.ts --target debug",
		"asbuild:release": "asc assembly/index.ts --target release",
		"asbuild": "npm run asbuild:release",
		"build": "npm run asbuild",
		"build:examples": "asc assembly/examples.ts --target release -o build/examples.wasm",
		"test": "node tests/test.js"
	},
	"devDependencies": {
		"assemblyscript": "^0.28.9"
	},
	"exports": {
		".": {
			"import": "./build/release.wasm"
		}
	}
}
